{
  "name": "react-responsive-carousel",
  "version": "3.0.13",
  "description": "React Responsive Carousel",
  "author": {
    "name": "Leandro Augusto Lemos",
    "url": "http://leandrowd.github.io/"
  },
  "main": "./lib/index.js",
  "license": "MIT",
  "keywords": [
    "react",
    "carousel",
    "gallery",
    "image-gallery",
    "slider",
    "responsive",
    "swipe",
    "mobile-friendly",
    "react-component",
    "view"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "node --harmony ./node_modules/gulp/bin/gulp",
    "jest": "jest --verbose",
    "test": "mocha \"src/**/*-test.js\" --compilers js:babel-core/register",
    "test-watch": "npm test -- --watch",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- \"src/**/*-test.js\" && babel-istanbul check-coverage",
    "prebuild": "npm test",
    "build": "babel ./src -d lib --ignore '__tests__' && gulp styles:package",
    "prepublish-to-npm": "git pull && npm run build && git commit -am 'Prepare for publishing'",
    "publish-to-npm": "(git pull origin master && npm version patch && git push origin master && git checkout release && git merge master && git push origin release && git checkout master && npm publish && git push --tags)",
    "prepublish-to-gh-pages": "node --harmony ./node_modules/gulp/bin/gulp prepublish",
    "publish-to-gh-pages": "node --harmony ./node_modules/gulp/bin/gulp publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leandrowd/react-responsive-carousel.git"
  },
  "bugs": {
    "url": "https://github.com/leandrowd/react-responsive-carousel/issues"
  },
  "homepage": "http://leandrowd.github.io/react-responsive-carousel/",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-istanbul": "^0.11.0",
    "babel-jest": "^14.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015-ie": "^6.6.2",
    "babel-preset-react": "^6.11.1",
    "browserify": "^12.0.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "connect-modrewrite": "^0.9.0",
    "enzyme": "^2.4.1",
    "gh-pages": "^0.11.0",
    "gulp": "^3.8.9",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.3.4",
    "gulp-connect": "~2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-cssmin": "^0.1.6",
    "gulp-if": "^1.2.4",
    "gulp-livereload": "^2.1.1",
    "gulp-notify": "^1.4.2",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.2.0",
    "gulp-shell": "^0.2.10",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.3.1",
    "gulp-util": "^3.0.0",
    "jest-cli": "^14.1.0",
    "mocha": "^3.0.2",
    "proxyquire": "^1.7.10",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "reactify": "^0.15.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^2.1.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-easy-swipe": "0.0.4"
  }
}
